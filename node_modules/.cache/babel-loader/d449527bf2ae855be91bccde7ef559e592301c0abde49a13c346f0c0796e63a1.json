{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport Menu from './components/menu/Menu';\nimport HamburgerMenu from './components/generic/HamburgerMenu.vue';\nexport default {\n  name: 'App',\n  components: {\n    hamburgerMenu: HamburgerMenu,\n    tmMenu: Menu\n  },\n  data() {\n    return {\n      openMenu: false\n    };\n  },\n  computed: {\n    documentTitle() {\n      return this.$t(this.$route.meta.title);\n    }\n  },\n  watch: {\n    openMenu(val) {\n      const body = document.body;\n      if (val === true) {\n        body.classList.add('no-scroll');\n      } else {\n        body.classList.remove('no-scroll');\n      }\n    }\n  },\n  methods: {\n    onOpenPage(event) {\n      this.openMenu = false;\n      this.$router.push({\n        name: event\n      });\n    }\n  }\n};","map":{"version":3,"mappings":";AAiBA;AACA;AAEA;EACAA;EACAC;IACAC;IACAC;EACA;EACAC;IACA;MACAC;IACA;EACA;EACAC;IACAC;MACA;IACA;EACA;EACAC;IACAH;MACA;MACA;QACAI;MACA;QACAA;MACA;IACA;EACA;EACAC;IACAC;MACA;MACA;QAAAX;MAAA;IACA;EACA;AACA","names":["name","components","hamburgerMenu","tmMenu","data","openMenu","computed","documentTitle","watch","body","methods","onOpenPage"],"sourceRoot":"src","sources":["App.vue"],"sourcesContent":["<template>\n  <div id=\"app\">\n    <hamburger-menu\n      :open-menu=\"openMenu\"\n      @menu=\"openMenu = $event\"\n    />\n    <router-view />\n    <transition name=\"slide-down\">\n      <tm-menu\n        v-if=\"openMenu === true\"\n        @open-page=\"onOpenPage($event)\"\n      />\n    </transition>\n  </div>\n</template>\n\n<script>\nimport Menu from './components/menu/Menu'\nimport HamburgerMenu from './components/generic/HamburgerMenu.vue'\n\nexport default {\n  name: 'App',\n  components: {\n    hamburgerMenu: HamburgerMenu,\n    tmMenu: Menu\n  },\n  data() {\n    return {\n      openMenu: false,\n    }\n  },\n  computed: {\n    documentTitle() {\n      return this.$t(this.$route.meta.title);\n    }\n  },\n  watch: {\n    openMenu(val) {\n      const body = document.body;\n      if (val === true) {\n        body.classList.add('no-scroll')\n      } else {\n        body.classList.remove('no-scroll')\n      }\n    }\n  },\n  methods: {\n    onOpenPage(event) {\n      this.openMenu = false;\n      this.$router.push({ name: event });\n    }\n  }\n};\n</script>\n\n<style lang=\"scss\">\n#app {\n  font-family: \"OpenSans-Regular\", Helvetica, sans-serif;\n  font-size: 16px;\n  color: $black;\n  overflow: hidden;\n}\n\n.hero-title {\n  font-family: 'YesevaOne-Regular', 'Tahoma', sans-serif;\n  font-size: 10rem;\n}\n\n.container {\n  max-width: 100% !important;\n  padding: 0 !important;\n}\n\n.slide-down-enter-active {\n  animation: new-animation .5s linear;\n}\n.slide-down-leave-active {\n  animation: new-animation .5s linear reverse;\n}\n\n@keyframes new-animation {\n  0% {\n    height: 0;\n    top: -400px;\n  }\n\n  50% {\n    height: 50vh;\n    top: -200px;\n  }\n\n  100% {\n    height: 100vh;\n    top: 0;\n  }\n}\n</style>\n"]},"metadata":{},"sourceType":"module","externalDependencies":[]}